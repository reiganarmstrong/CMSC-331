banana {
   var{int} x ;

   x := 4 ;
   print "x is " ;
   println x ;
   if ( x <= 5 ) then {
      println "Yes, x is less than or equal to 5" ;
   } else {
      println "No, x is not less than or equal to 5" ;
   }

   x := 9 ;
   print "x is " ;
   println x ;
   if ( x <= 5 ) then {
      println "Yes, x is less than or equal to 5" ;
   } else {
      println "No, x is not less than or equal to 5" ;
   }

   println "The End." ;
}
